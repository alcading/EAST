<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resources.bcms.data.pub">
	<class name="BcmsRequestInfo" table="Bcms_RequestInfo">
		<meta attribute="sync-DAO">false</meta>
		<id
			name="deliveryType"
			type="string"
			column="deliveryType"
		>
		</id>

		<property name="deliveryName" column="deliveryName" type="string" length="500" />

		<property name="requestURL" column="requestURL" type="string" length="500" />
		
		<property name="requestMethod" column="requestMethod" type="string" length="10" />

		<property name="preRequest" column="preRequest" type="string" length="10" />


	</class>
</hibernate-mapping>