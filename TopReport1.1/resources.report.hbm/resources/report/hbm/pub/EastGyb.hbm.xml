<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resources.east.data.pub">
	<class name="EastGyb" table="EAST_GYB">
		<!-- 机构信息表 -->
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="EastGybPK">
			<!-- 柜员号 -->
			<key-property name="gyh" column="gyh" type="string" />
			<!-- 内部机构号 -->
			<key-property name="nbjgh" column="nbjgh" type="string" />
			<!-- 采集日期 -->
			<key-property name="cjrq" column="cjrq" type="string" />
		</composite-id>
		
		<!-- 工号 -->
		<property name="gh" column="gh" type="string" length="30" />
		<!-- 银行机构代码 -->
		<property name="yxjgdm" column="yxjgdm" type="string" length="30" />
		
		<!-- 总行机构代码 -->
		<property name="zxjgdm" column="zxjgdm" type="string" length="30" />

		<!-- 金融许可证号 -->
		<property name="jrxkzh" column="jrxkzh" type="string" length="30" />

		<!-- 银行机构名称 -->
		<property name="yxjgmc" column="yxjgmc" type="string" length="200" />

		<!-- 柜员类型 -->
		<property name="gylx" column="gylx" type="string" length="30" />
		<!--是否实体柜员 -->
		<property name="sfstgy" column="sfstgy" type="string" length="4" />
		<!--客户经理标志 -->
		<property name="khjlbz" column="khjlbz" type="string" length="4" />
		<!--经办财务标志 -->
		<property name="jbzwbz" column="jbzwbz" type="string" length="4" />
		<!--权限管理标志 -->
		<property name="qxglbz" column="qxglbz" type="string" length="4" />
		<!--一般管理标志 -->
		<property name="ybglbz" column="ybglbz" type="string" length="4" />
		<!--信贷员标志 -->
		<property name="xdybz" column="xdybz" type="string" length="4" />
		<!--库管员标志 -->
		<property name="kgybz" column="kgybz" type="string" length="4" />
		<!--综合柜员标志 -->
		<property name="zhgybz" column="zhgybz" type="string" length="4" />
		<!--负授权标志 -->
		<property name="sqbz" column="sqbz" type="string" length="4" />
		<!--授权范围 -->
		<property name="sqfw" column="sqfw" type="string" length="60" />
		<!--岗位编号 -->
		<property name="gwbh" column="gwbh" type="string" length="60" />
		<!--柜员用户级别 -->
		<property name="gyyhjb" column="gyyhjb" type="string" length="30" />
		<!--柜员权限级别 -->
		<property name="gyqxjb" column="gyqxjb" type="string" length="30" />
		<!--上岗日期 -->
		<property name="sgrq" column="sgrq" type="string" length="8" />
		<!--柜员状态 -->
		<property name="gwzt" column="gwzt" type="string" length="30" />
		
	</class>
</hibernate-mapping>