		<Field id="recStatus" desc="记录状态" require="false" edittype="select"
			datatype="string" method="None" readonly="false" size="2" translated="DATA_DIC.62"
			status="N" default="" xpath="/recStatus" />

		<Field id="repStatus" desc="回执状态" require="false" edittype="select"
			datatype="string" method="None" readonly="false" size="2" translated="DATA_DIC.63"
