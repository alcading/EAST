			default="" xpath="/approveResult" />

		<Field id="workDate" desc="工作日期" require="false" edittype="date"
			datatype="date" method="None" readonly="false" size="32" status="N"
			default="" xpath="/workDate" />

		<Field id="subSuccess" desc="是否已成功上报" require="false" edittype="text"
			datatype="string" method="None" readonly="false" size="2" status="N"
			default="" xpath="/subSuccess" />

		<Field id="crtuser" desc="填报人" require="" edittype="text"
			datatype="string" method="None" readonly="false" size="20" status="N"
			default="" xpath="/crtuser" />


	</Fields>

	<Where>

		 <DateTextBox id="qworkDateStart" desc="工作日期开始" tip="工作日期开始"
			require="" status="F" datatype="date" method="None" size="150" width=""
			primary="false" index="0" type="String" />

		<DateTextBox id="qworkDateEnd" desc="工作日期结束" tip="工作日期结束"
			require="" status="F" datatype="date" method="None" size="150" width=""
			primary="false" index="1" type="String" />


		<Option id="qapproveStatus" desc="审核状态" require="false" edittype="select" size="2" status="N"
			translated="DATA_DIC.64" datatype="string" default="" xpath="/notCollectFlag" index="5"/>

		<Option id="qrepStatus" desc="回执状态" require="false" edittype="select" size="2" status="N"
			translated="DATA_DIC.63" datatype="string" default=""  index="6"/>


		<Option id="qrecStatus" desc="记录状态" require="false" edittype="select" size="2" status="N"
