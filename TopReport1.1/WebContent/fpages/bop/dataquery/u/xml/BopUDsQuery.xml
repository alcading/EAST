				require="false" readOnly="" index="2"/>


		<TextBox id="qcustcode" desc="组织机构代码" edittype="text" status="F"
			datatype="string" size="9" index="6" />

		<TextBox id="qcustname" desc="组织机构名称" edittype="text" status="F"
