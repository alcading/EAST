	 		size="32" status="F" default="" xpath="/othacc"  />

		<Field id="method" desc="结算方式" edittype="select" datatype="string" method="None" translated="DATA_DIC.76"
	 	  readonly="false" size="1" status="F" default="" xpath="/method" />

