<?xml version="1.0" encoding="UTF-8"?>
<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="数据分析" navigate="数据分析  &gt; 数据分析  &gt; 数据分析详细  " type="call"
	interface="true" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	getterclassname="com.huateng.report.getter.DataDetailRecordGetter">
	<Include id="BankParam" />

	<PageQryExp type="XLS" limit="true" encoding="" url="/trans/qryExp.do" desc="数据导出-数据分析结果"  />

	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		<Field id="id" desc="主键" require="" edittype="text" datatype="string" method="None"
		  readonly="true" size="32" default=""   primary="true" xpath="/id" />

		<Field id="analyNo" desc="分析编号" require="" edittype="text" datatype="string" method="None"
		  readonly="true" size="32" default=""   primary="true" xpath="/analyNo" />

		<Field id="confSeq" desc="执行顺序" require="" edittype="text" datatype="int" method="None"
		  readonly="true" size="8" default=""   primary="true" xpath="/confSeq" />

		<Field id="confDesc" desc="执行描述" require="" edittype="text" datatype="string" method="None"
		  readonly="true" size="256" default=""   primary="true" xpath="/confDesc" />

	   <Field id="startTm" desc="开始时间" require="" edittype="text" datatype="string" method="None"
		  readonly="true" size="20" status="F" default=""  primary="true" xpath="/startTm" />

		<Field id="endTm" desc="结束时间" require="" edittype="text" datatype="string" method="None"
		  readonly="true" size="20" status="F" default=""  primary="true" xpath="/endTm" />

		<Field id="execSta" desc="状态" require="" edittype="select"
			datatype="string" method="None" readonly="false" size="2" status="F"
			translated="LIST:00,未执行;01,执行中;02,已完成" default="" primary="true" xpath="/execSta" />

		<Field id="executeResult" desc="结果" require="" edittype="select"
			datatype="string" method="None" readonly="false" size="2" status="F"
			translated="LIST:00,执行中;01,执行成功;02,执行失败;03,未执行" default="" primary="true" xpath="/executeResult" />

		<Field id="execRemark" desc="执行备注" require="" edittype="text" datatype="string" method="None"
		  readonly="true" size="512" status="F" default="" primary="true" xpath="/execRemark" />

       </Fields>

	<Operations>
	    <!-- 岗位信息  -->
	    <Button id="btClose" name="btClose" desc="关闭" operation="syncsubmit"
			url="#" txn="" />
	</Operations>

	<Script></Script>

</CommQry>