<?xml version="1.0" encoding="UTF-8"?>
<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->

<CommQry title="CurrencyManEntry.title" navigate="基础信息管理  &gt; 币种信息维护"
    type="call" interface = "false" pagesize="10" async="true" databusid=""
    transdataactionurl="/trans/TransDataAction.do" txnlogflag="true"
    getterclassname="com.huateng.report.basis.getter.CurrencyManEntryDetailGetter" >
<Include id="BankParam" />

<PageQryExp type="XLS" limit="true" encoding="" url="/trans/qryExp.do" desc="批量下载-数据字典" perfetch="15" />
<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->

		<Field id="old_id" desc="CurrencyManEntry.field.id" edittype="text" require="true" size="6" errmsg="币种代码不能为空！"  datatype="string" readonly="true" status="F" xpath="old_currency/id"  />
		<Field id="id" desc="CurrencyManEntry.field.id" edittype="text" require="true" size="6" errmsg="币种代码不能为空！"  datatype="string"  readonly="true" status="F" xpath="currency/id"  />
		
		<Field id="old_currencyName" desc="CurrencyManEntry.field.currencyName" edittype="text" size="52" require="true" errmsg="币种名称不能为空！" datatype="string" readonly="true" status="F" xpath="old_currency/currencyName" />
		<Field id="currencyName" desc="CurrencyManEntry.field.currencyName" edittype="text" size="52" require="true" errmsg="币种名称不能为空！" datatype="string"  readonly="true" status="F" xpath="currency/currencyName" />
		
		<Field id="old_dotNum" desc="CurrencyManEntry.field.dotNum" rules="isNum" require="true" errmsg="小数点尾数不能为空且要大于0" size="2" default="2"  edittype="text" datatype="int" readonly="true" status="F" xpath="old_currency/dotNum" />
		<Field id="dotNum" desc="CurrencyManEntry.field.dotNum" rules="isNum" require="true" errmsg="小数点尾数不能为空且要大于0" size="2" default="2"  edittype="text" datatype= "int"  readonly="true" status="F" xpath="currency/dotNum" />		
		
</Fields>

<Operations>
   <Button id="btClose" name="btClose" desc="返回" operation="syncsubmit"
			url="#" txn="" />
    
    </Operations>

	<Script></Script>
 </CommQry>