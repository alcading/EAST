<CommQry title="外汇账户内结汇—基础信息" navigate="BOP-国际收支&gt; 外汇账户内结汇补录 &gt; 基础信息"
	type="call" 
	interface="true" 
	pagesize="10" 
	async="true" 
	databusid=""
	transdataactionurl="/trans/TransDataAction.do" 
	getterclassname="com.huateng.report.jsh.dataquery.getter.JshDDsQueryGetter">
	<Include id="BankParam" />
	<PageQryExp type="XLS" limit="true" encoding="" url="/trans/qryExp.do" desc="批量下载-外汇账户内结汇(查询)-基础信息" />
	<Fields>
