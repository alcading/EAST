<?xml version="1.0" encoding="UTF-8"?>

<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="涉农统计表" navigate="银监标准化    &gt; 涉农统计表" type="call"
	interface="false" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true"
	getterclassname="resources.east.data.getter.EastSntjbGetter">
	<Include id="BankParam" />
	<!--
	<PageQryExp type="CSV" limit="true" encoding="GBK" url="/trans/qryExp.do" directory="F:/upload"/>
	-->
	<PageQryExp type="XLS" limit="true" encoding="GBK" url="/trans/qryExp.do" desc="批量下载-涉农统计表" />
	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		
		<Field id="id" desc="工号" edittype="text" datatype="string" size="20" xpath="/id"></Field>
		<Field id="jrjgdm" desc="金融机构代码" require="true"  edittype="text" status="F" datatype="string" size="30" xpath="/jrjgdm"></Field>
		<Field id="jrxkzh" desc="金融许可证号" require="true"  edittype="text" status="F" datatype="string" size="30" xpath="/jrxkzh"></Field>
		<Field id="nbjgh" desc="内部机构号" require="true"  edittype="text" status="F" datatype="string" size="30" xpath="/id/nbjgh"></Field>
		<Field id="tjkmbh" desc="统计科目编号" require="true"  edittype="text" status="F" datatype="string" size="20" xpath="/id/tjkmbh"></Field>
		<Field id="tjkmmc" desc="统计科目名称"  require="true" edittype="text" status="F" datatype="string" size="120" xpath="/tjkmmc"></Field>
		<Field id="tjkmje" desc="统计科目金额"  require="true" edittype="text" status="F" datatype="currency" size="20" xpath="/tjkmje"></Field>
		<Field id="qxlx" desc="期限类型"  require="true" edittype="select" translated="DATA_DIC.20080" status="F" datatype="string" size="6" xpath="/id/qxlx"></Field>
		<Field id="tjrq" desc="统计日期" require="true"  edittype="text" status="F" datatype="string" size="8" xpath="/id/tjrq"></Field>
		<Field id="bz" desc="币种" require="true"  edittype="text" status="F" datatype="string" size="3" xpath="/id/bz"></Field>
		<Field id="dqmc" desc="地区名称" require="true"  edittype="text" status="F" datatype="string" size="60" xpath="/dqmc"></Field>
		<Field id="xzqhdm" desc="行政区划代码"  require="true" edittype="text" status="F" datatype="string" size="20" xpath="/xzqhdm"></Field>
		<Field id="cjrq" desc="采集日期"  require="true" edittype="text" status="F" datatype="string" size="8" xpath="/id/cjrq"></Field>
		<Field id="operation" desc="操作" status="N" method="None" xpath=""
			   primary="true" type="" size="20" default="" tip="操作" require=""/>	
       </Fields>
    <Where>
    	<TextBox id="cjrq" desc="采集日期" datatype="date" index="5" require="true" errmsg="数据日期不能为空！" tip="时间为必输项" size="12"/>
    	<TextBox id="tjkmbh" desc="统计科目编号" datatype="string" index="3" size="20"/>
    	<TextBox id="tjkmmc" desc="统计科目名称" datatype="string" index="3" size="120"/>
    	<TextBox id="qxlx" desc="期限类型" datatype="string" index="3" size="20"/>
    </Where>
	<Operations>
		<Button id="btModOrAdd" name="btModOrAdd" desc="保存" operation="asysubmit"
			updateClass="resources.east.data.update.EastSntjbModOrAddUpdate"
			url="" txn="" />
		<Button id="btDel" name="btDel" desc="删除" operation="asysubmit"
			updateClass="resources.east.data.update.EastSntjbModOrAddUpdate"
			url="" />
		<Button id="btCancel" name="btCancel" desc="取消" operation="href" url="" />
		<Button id="btAdd" name="btAdd" desc="新增" operation="" url="#" show="" group="" txn="" icon="icon-add"/>
	</Operations>
</CommQry>