<?xml version="1.0" encoding="UTF-8"?>

<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="信贷业务担保合同" navigate="银监标准化    &gt; 信贷业务担保合同" type="call"
	interface="false" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true"
	getterclassname="resources.east.data.getter.EastXdywdbhtGetter">
	<Include id="BankParam" />
	<!--
	<PageQryExp type="CSV" limit="true" encoding="GBK" url="/trans/qryExp.do" directory="F:/upload"/>
	-->
	<PageQryExp type="XLS" limit="true" encoding="GBK" url="/trans/qryExp.do" desc="批量下载-信贷业务担保合同" />
	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		
		
		<Field id="id" desc="id" edittype="text" datatype="string" size="30" xpath="/id"></Field>
		<Field id="dbhth" desc="担保合同号" status="F"  edittype="text"  datatype="string" size="100" xpath="/id/dbhth" />
		<Field id="jrxkzh" desc="金融许可证号" edittype="text" status="F" datatype="string" size="30" xpath="/jrxkzh"></Field>
		<Field id="yxjgdm" desc="银行机构代码" status="F"  edittype="text"  datatype="string" size="30" xpath="/yxjgdm"></Field>
		<Field id="nbjgh" desc="内部机构号 " status="F"  edittype="text"  datatype="string" size="30" xpath="/nbjgh" />
		<Field id="dblx" desc="担保类型" status="F"  edittype="text"  datatype="string" size="30" xpath="/dblx" />
	    <Field id="dbhtlx" desc="担保合同类型" status="F" edittype="text" datatype="string" size="30" xpath="/dbhtlx"></Field>	
		<Field id="bzrlb" desc="保证人类别" status="F"  edittype="text"  datatype="string" size="30" xpath="/bzrlb" />
		<Field id="bzrmc" desc="保证人名称" status="F"  edittype="text"  datatype="string" size="200" xpath="/bzrmc" />
		<Field id="zjlb" desc="证件类别" status="F"  edittype="text"  datatype="string" size="60" xpath="/zjlb" />
		<Field id="zjhm" desc="证件号码" status="F"  edittype="text"  datatype="string" size="200" xpath="/id/zjhm" />
		<Field id="bzrjzc" desc="保证人净资产" status="F"  edittype="text"  datatype="string" size="24" xpath="/bzrjzc" />
		<Field id="dbqsrq" desc="担保起始日期" status="F"  edittype="text"  datatype="string" size="8" xpath="/dbqsrq" />
		<Field id="dbdqrq" desc="担保到期日期" status="F"  edittype="text"  datatype="string" size="8" xpath="/dbdqrq" />
		<Field id="dbhtzt" desc="担保合同状态" status="F"  edittype="text"  datatype="string" size="30" xpath="/dbhtzt" />
		<Field id="dbhtqdrq" desc="担保合同签订日期" status="F"  edittype="text"  datatype="string" size="8" xpath="/dbhtqdrq" />
		<Field id="dbhtsxrq" desc="担保合同生效日期" status="F"  edittype="text"  datatype="string" size="8" xpath="/dbhtsxrq" />
		<Field id="dbhtdqrq" desc="担保合同到期日期" status="F"  edittype="text"  datatype="string" size="8" xpath="/dbhtdqrq" />
		<Field id="dbbz" desc="担保币种" status="F"  edittype="text"  datatype="string" size="3" xpath="/dbbz" />
		<Field id="dbzje" desc="担保总金额" status="F"  edittype="text"  datatype="string" size="24" xpath="/dbzje" />
		<Field id="dbwpxh" desc="担保物品顺序" status="F"  edittype="text"  datatype="string" size="1000" xpath="/dbwpxh" />
		<Field id="jlygh" desc="建立员工号" status="F"  edittype="text"  datatype="string" size="30" xpath="/jlygh" />
		<Field id="cjrq" desc="采集日期" status="F"  edittype="text"  datatype="string" size="8" xpath="/id/cjrq" />
		<Field id="operation" desc="操作" status="N" method="None" xpath=""
			   primary="true" type="" size="20" default="" tip="操作" require=""/>	
       </Fields>
	<Where>
    	<TextBox id="cjrq" desc="采集日期" datatype="date" index="5" require="true" errmsg="数据日期不能为空！" tip="时间为必输项" size="12"/>
    	<TextBox id="yxjgdm" desc="银行机构代码" datatype="string" index="3" size="30"/>
    </Where>
	<Operations>
		<Button id="btModOrAdd" name="btModOrAdd" desc="保存" operation="asysubmit"
			updateClass="resources.east.data.update.EastXdywdbhtModOrAddUpdate"
			url="" txn="" />
		<Button id="btDel" name="btDel" desc="删除" operation="asysubmit"
			updateClass="resources.east.data.update.EastXdywdbhtModOrAddUpdate"
			url="" />
		<Button id="btCancel" name="btCancel" desc="取消" operation="href" url="" />
		<Button id="btAdd" name="btAdd" desc="新增" operation="" url="#" show="" group="" txn="" icon="icon-add"/>
	</Operations>
</CommQry>