<?xml version="1.0" encoding="UTF-8"?>

<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="客户理财账户信息表" navigate="银监标准化    &gt; 客户理财账户信息表" type="call"
	interface="false" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true"
	getterclassname="resources.east.data.getter.EastKhlczhxxbGetter">
	<Include id="BankParam" />
	<!--
	<PageQryExp type="CSV" limit="true" encoding="GBK" url="/trans/qryExp.do" directory="F:/upload"/>
	-->
	<PageQryExp type="XLS" limit="true" encoding="GBK" url="/trans/qryExp.do" desc="批量下载-客户理财账户信息表" />
	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		
		<Field id="id" desc="理财账号" edittype="text" datatype="string" size="60" xpath="/id"></Field>
		<Field id="yxjgdm" desc="银行机构代码"  require="true" edittype="text" status="F" datatype="string" size="30" xpath="/yxjgdm"></Field>
		<Field id="jrxkzh" desc="金融许可证号" require="true"  edittype="text" status="F" datatype="string" size="30" xpath="/jrxkzh"></Field>
		<Field id="nbjgh" desc="内部机构号"  require="true" edittype="text" status="F" datatype="string" size="30" xpath="/nbjgh"></Field>
		<Field id="mxkmbh" desc="明细科目编号"  require="true" edittype="text" status="F" datatype="string" size="60" xpath="/mxkmbh"></Field>
		<Field id="yxjgmc" desc="银行机构名称"  require="true" edittype="text" status="F" datatype="string" size="200" xpath="/yxjgmc"></Field>
		<Field id="mxkmmc" desc="明细科目名称"  require="true" edittype="text" status="F" datatype="string" size="300" xpath="/mxkmmc"></Field>
		<Field id="bz" desc="币种"  require="true" edittype="text" status="F" datatype="string" size="3" xpath="/bz"></Field>
		<Field id="lczh" desc="理财账号" require="true"  edittype="text" status="F" datatype="string" size="60" xpath="/id/lczh"></Field>
		<Field id="khtybh" desc="客户统一编号"  require="true" edittype="text" status="F" datatype="string" size="60" xpath="/khtybh"></Field>
		<Field id="khxm" desc="客户姓名" require="true"  edittype="text" status="F" datatype="string" size="100" xpath="/khxm"></Field>
		<Field id="glhqckzh" desc="关联活期存款账号" require="true"  edittype="text" status="F" datatype="string" size="60" xpath="/id/glhqckzh"></Field>
		<Field id="lccpmc" desc="理财产品名称" require="true"  edittype="text" status="F" datatype="string" size="200" xpath="/lccpmc"></Field>
		<Field id="hnbsm" desc="行内标识码"  require="true" edittype="text" status="F" datatype="string" size="30" xpath="/id/hnbsm"></Field>
		<Field id="fezs" desc="份额总数"  require="true" edittype="text" status="F" datatype="string" size="20" xpath="/fezs"></Field>
		<Field id="djfe" desc="冻结份额" require="true"  edittype="text" status="F" datatype="string" size="20" xpath="/djfe"></Field>
		<Field id="hlztzbz" desc="再投资标志"  require="true" edittype="select" translated="DATA_DIC.20034" status="F" datatype="string" size="4" xpath="/hlztzbz"></Field>
		<Field id="bqsy" desc="本期收益"  require="true" edittype="text" status="F" datatype="currency" size="20" xpath="/bqsy"></Field>
		<Field id="ljsy" desc="累计收益"  require="true" edittype="text" status="F" datatype="currency" size="20" xpath="/ljsy"></Field>
		<Field id="mrcb" desc="买入成本"  require="true" edittype="text" status="F" datatype="currency" size="20" xpath="/mrcb"></Field>
		<Field id="bqqsrq" desc="本期起始日期"  require="true" edittype="text" status="F" datatype="string" size="8" xpath="/bqqsrq"></Field>
		<Field id="bqdqrq" desc="本期到期日期" require="true"  edittype="text" status="F" datatype="string" size="8" xpath="/bqdqrq"></Field>
		<Field id="khrq" desc="开户日期"  require="true" edittype="text" status="F" datatype="string" size="8" xpath="/khrq"></Field>
		<Field id="scdhrq" desc="上次动户日期"  require="true" edittype="text" status="F" datatype="string" size="8" xpath="/scdhrq"></Field>
		<Field id="cjrq" desc="采集日期"  require="true" edittype="text" status="F" datatype="string" size="8" xpath="/cjrq"></Field>
		<Field id="operation" desc="操作" status="N" method="None" xpath=""
			   primary="true" type="" size="20" default="" tip="操作" require=""/>	
       </Fields>
    <Where>
    	<TextBox id="cjrq" desc="采集日期" datatype="date" index="5" require="true" errmsg="数据日期不能为空！" tip="时间为必输项" size="12"/>
    	<TextBox id="lczh" desc="理财账号" datatype="string" index="3" size="60"/>
    	<TextBox id="glhqckzh" desc="关联活期存款账号" datatype="string" index="3" size="60"/>
    	<TextBox id="hnbsm" desc="行内标识码" datatype="string" index="3" size="30"/>
    </Where>
	<Operations>
		<Button id="btModOrAdd" name="btModOrAdd" desc="保存" operation="asysubmit"
			updateClass="resources.east.data.update.EastKhlczhxxbModOrAddUpdate"
			url="" txn="" />
		<Button id="btDel" name="btDel" desc="删除" operation="asysubmit"
			updateClass="resources.east.data.update.EastKhlczhxxbModOrAddUpdate"
			url="" />
		<Button id="btCancel" name="btCancel" desc="取消" operation="href" url="" />
		<Button id="btAdd" name="btAdd" desc="新增" operation="" url="#" show="" group="" txn="" icon="icon-add"/>
	</Operations>
</CommQry>