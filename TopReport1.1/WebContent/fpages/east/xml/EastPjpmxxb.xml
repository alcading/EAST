<?xml version="1.0" encoding="UTF-8"?>

<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="票据票面信息表" navigate="银监标准化    &gt; 票据票面信息表" type="call"
	interface="false" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true"
	getterclassname="resources.east.data.getter.EastPjpmxxbGetter">
	<Include id="BankParam" />
	<!--
	<PageQryExp type="CSV" limit="true" encoding="GBK" url="/trans/qryExp.do" directory="F:/upload"/>
	-->
	<PageQryExp type="XLS" limit="true" encoding="GBK" url="/trans/qryExp.do" desc="批量下载-票据票面信息表" />
	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->


		<Field id="id" desc="id" edittype="text" datatype="string" size="30" xpath="/id"></Field>
		<Field id="pjhm" desc="票据号码"  require="true" edittype="text" status="F" datatype="string" size="60" xpath="/id/pjhm"></Field>	
		<Field id="cprbh" desc="出票人编号"  require="true" edittype="text" status="F" datatype="string" size="60" xpath="/cprbh"></Field>
	    <Field id="fkxdm" desc="付款行代码"  require="true" status="F" edittype="text" datatype="string" size="30" xpath="/fkxdm"></Field>	
		<Field id="yxjgdm" desc="银行机构代码"  require="true" status="F"  edittype="text"  datatype="string" size="30" xpath="/yxjgdm"></Field>
		<Field id="jrxkzh" desc="金融许可证号"  require="true" status="F"  edittype="text"  datatype="string" size="30" xpath="/jrxkzh" />
		<Field id="nbjgh" desc="内部机构号 "  require="true" status="F"  edittype="text"  datatype="string" size="30" xpath="/nbjgh" />
		<Field id="pjlx" desc="票据类型"  require="true" status="F"  edittype="select" translated="DATA_DIC.20058"  datatype="string" size="30" xpath="/pjlx" />
		<Field id="cprqc" desc="出票人全称"  require="true" status="F"  edittype="text"  datatype="string" size="200" xpath="/cprqc" />
		<Field id="cprzh" desc="出票人账号"  require="true" status="F"  edittype="text"  datatype="string" size="60" xpath="/cprzh" />
		<Field id="fkxmc" desc="付款行名称" require="true"  status="F"  edittype="text"  datatype="string" size="200" xpath="/fkxmc" />
		<Field id="bz" desc="币种"  require="true" status="F"  edittype="text"  datatype="string" size="3" xpath="/bz" />
		<Field id="pmje" desc="票面金额"  require="true" status="F"  edittype="text"  datatype="currency" size="24" xpath="/pmje" />
		<Field id="qfrq" desc="签发日期"  require="true" status="F"  edittype="text"  datatype="string" size="8" xpath="/qfrq" />
		<Field id="pjdqrq" desc="票据到期日期" require="true"  status="F"  edittype="text"  datatype="string" size="8" xpath="/pjdqrq" />
		<Field id="skrmc" desc="收款人名称"  require="true" status="F"  edittype="text"  datatype="string" size="200" xpath="/skrmc" />
		<Field id="skrzh" desc="收款人账号" require="true"  status="F"  edittype="text"  datatype="string" size="60" xpath="/skrzh" />
		<Field id="skrkhx" desc="收款人开户行"  require="true" status="F"  edittype="text"  datatype="string" size="30" xpath="/skrkhx" />
		<Field id="syfphm" desc="商业发票号码" require="true"  status="F"  edittype="text"  datatype="string" size="200" xpath="/syfphm" />
		<Field id="syfpbz" desc="商业发票币种"  require="true" status="F"  edittype="text"  datatype="string" size="3" xpath="/syfpbz" />
		<Field id="syfpje" desc="商业发票金额"  require="true" status="F"  edittype="text"  datatype="currency" size="24" xpath="/syfpje" />
		<Field id="syfpzl" desc="商业发票种类"  require="true" status="F"  edittype="select" translated="DATA_DIC.20059"  datatype="string" size="60" xpath="/syfpzl" />
		<Field id="cjrq" desc="采集日期"  require="true" status="F"  edittype="text"  datatype="string" size="8" xpath="/id/cjrq" />
		<Field id="operation" desc="操作" status="N" method="None" xpath=""
			   primary="true" type="" size="20" default="" tip="操作" require=""/>	
       </Fields>
	<Where>
    	<TextBox id="cjrq" desc="采集日期" datatype="date" index="5" require="true" errmsg="数据日期不能为空！" tip="时间为必输项" size="12"/>
    	<TextBox id="yxjgdm" desc="银行机构代码" datatype="string" index="3" size="30"/>
    </Where>
	<Operations>
		<Button id="btModOrAdd" name="btModOrAdd" desc="保存" operation="asysubmit"
			updateClass="resources.east.data.update.EastXmdkxxbModOrAddUpdate"
			url="" txn="" />
		<Button id="btDel" name="btDel" desc="删除" operation="asysubmit"
			updateClass="resources.east.data.update.EastXmdkxxbModOrAddUpdate"
			url="" />
		<Button id="btCancel" name="btCancel" desc="取消" operation="href" url="" />
		<Button id="btAdd" name="btAdd" desc="新增" operation="" url="#" show="" group="" txn="" icon="icon-add"/>
	</Operations>
</CommQry>