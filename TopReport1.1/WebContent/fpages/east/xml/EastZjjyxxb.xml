<CommQry title="资金交易信息表" navigate="银监标准化    &gt; 资金交易信息表" type="call"
	interface="false" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true"
	getterclassname="resources.east.data.getter.EastZcfzkmtjbGetter">
	<Include id="BankParam" />
	<!--
	<PageQryExp type="CSV" limit="true" encoding="GBK" url="/trans/qryExp.do" directory="F:/upload"/>
	-->
	<PageQryExp type="XLS" limit="true" encoding="GBK" url="/trans/qryExp.do" desc="批量下载-资金交易信息表表" />
	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		
		<Field id="id" desc="交易编号" edittype="text" datatype="string" size="40" xpath="/id"></Field>
		<Field id="yxjgdm" desc="银行机构代码" edittype="text" status="F" datatype="string" size="30" xpath="/yxjgdm"></Field>
		<Field id="jrxkzh" desc="金融许可证号" edittype="text" status="F" datatype="string" size="30" xpath="/jrxkzh"></Field>
		<Field id="nbjgh" desc="内部机构号" edittype="text" status="F" datatype="string" size="30" xpath="/nbjgh"></Field>
		<Field id="mxkmbh" desc="明细科目编号" edittype="text" status="F" datatype="string" size="60" xpath="/mxkmbh"></Field>
		<Field id="yxjgmc" desc="银行机构名称" edittype="text" status="F" datatype="string" size="200" xpath="/yxjgmc"></Field>
		<Field id="mxkmmc" desc="明细科目名称" edittype="text" status="F" datatype="string" size="300" xpath="/mxkmmc"></Field>
		<Field id="jybh" desc="交易编号" edittype="text" status="F" datatype="string" size="40" xpath="/id/jybh"></Field>
		<Field id="lccpdjbm" desc="理财产品登记编码" edittype="text" status="F" datatype="string" size="30" xpath="/lccpdjbm"></Field>
		<Field id="jylx" desc="资金交易类型" edittype="text" status="F" datatype="string" size="6" xpath="/jylx"></Field>
		<Field id="jyzl" desc="资金交易子类" edittype="text" status="F" datatype="string" size="60" xpath="/jyzl"></Field>
