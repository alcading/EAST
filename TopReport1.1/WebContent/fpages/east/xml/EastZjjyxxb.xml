		<Field id="jyzhlx" desc="账户类型" edittype="text" status="F" datatype="string" size="12" xpath="/jyzhlx"></Field>
		<Field id="hth" desc="合同号" edittype="text" status="F" datatype="string" size="100" xpath="/hth"></Field>
		<Field id="htje" desc="合同金额" edittype="text" status="F" datatype="big_decimal" size="20" xpath="/htje"></Field>
		<Field id="bz" desc="币种" edittype="text" status="F" datatype="string" size="3" xpath="/bz"></Field>
		<Field id="jczckhmc" desc="基础资产客户名称" edittype="text" status="F" datatype="string" size="200" xpath="/jczckhmc"></Field>
		<Field id="jczcsshy" desc="基础资产所属行业" edittype="text" status="F" datatype="string" size="100" xpath="/jczcsshy"></Field>
		<Field id="jczcsfwbhkh" desc="基础资产是否为本行客户" edittype="text" status="F" datatype="string" size="4" xpath="/jczcsfwbhkh"></Field>
		<Field id="jczczxfs" desc="基础资产增信方式" edittype="text" status="F" datatype="string" size="30" xpath="/jczczxfs"></Field>
		<Field id="jczczxr" desc="基础资产增信人" edittype="text" status="F" datatype="string" size="200" xpath="/jczczxr"></Field>
		<Field id="jygy" desc="交易柜员" edittype="text" status="F" datatype="string" size="100" xpath="/jygy"></Field>
		<Field id="spr" desc="审批人" edittype="text" status="F" datatype="string" size="100" xpath="/spr"></Field>
		<Field id="jydsdm" desc="交易对手代码" edittype="text" status="F" datatype="string" size="60" xpath="/jydsdm"></Field>
		<Field id="jydsmc" desc="交易对手名称" edittype="text" status="F" datatype="string" size="200" xpath="/jydsmc"></Field>
		<Field id="jyrq" desc="交易日期" edittype="text" status="F" datatype="string" size="8" xpath="/jyrq"></Field>
		<Field id="qsrq" desc="起始日期" edittype="text" status="F" datatype="string" size="8" xpath="/qsrq"></Field>
		<Field id="dqrq" desc="到期日期" edittype="text" status="F" datatype="string" size="8" xpath="/dqrq"></Field>
		<Field id="mmbz" desc="买卖标志" edittype="text" status="F" datatype="string" size="6" xpath="/mmbz"></Field>
		<Field id="jyqbz" desc="即远期标志" edittype="text" status="F" datatype="string" size="6" xpath="/jyqbz"></Field>
		<Field id="mrbz" desc="买入币种" edittype="text" status="F" datatype="string" size="3" xpath="/mrbz"></Field>
		<Field id="mrje" desc="买入金额" edittype="text" status="F" datatype="big_decimal" size="20" xpath="/mrje"></Field>
		<Field id="mcbz" desc="卖出币种" edittype="text" status="F" datatype="string" size="3" xpath="/mcbz"></Field>
		<Field id="mcje" desc="卖出金额" edittype="text" status="F" datatype="big_decimal" size="20" xpath="/mcje"></Field>
		<Field id="cjjg" desc="成交价格" edittype="text" status="F" datatype="string" size="100" xpath="/cjjg"></Field>
		<Field id="ywzt" desc="交易状态" edittype="text" status="F" datatype="string" size="20" xpath="/ywzt"></Field>
		<Field id="fhrq" desc="复核日期" edittype="text" status="F" datatype="string" size="8" xpath="/fhrq"></Field>
		<Field id="qxrq" desc="取消日期" edittype="text" status="F" datatype="string" size="8" xpath="/qxrq"></Field>
		<Field id="sjjgrq" desc="实际交割日期" edittype="text" status="F" datatype="string" size="8" xpath="/sjjgrq"></Field>
		<Field id="qsbz" desc="清算标志" edittype="text" status="F" datatype="string" size="4" xpath="/qsbz"></Field>
		<Field id="jfzh" desc="借方账号" edittype="text" status="F" datatype="string" size="60" xpath="/jfzh"></Field>
		<Field id="dfzh" desc="贷方账号" edittype="text" status="F" datatype="string" size="60" xpath="/dfzh"></Field>
		<Field id="jfje" desc="借方金额" edittype="text" status="F" datatype="big_decimal" size="20" xpath="/jfje"></Field>
		<Field id="dfje" desc="贷方金额" edittype="text" status="F" datatype="big_decimal" size="20" xpath="/dfje"></Field>
		<Field id="jfbz" desc="借方币种" edittype="text" status="F" datatype="string" size="3" xpath="/jfbz"></Field>
		<Field id="dfbz" desc="贷方币种" edittype="text" status="F" datatype="string" size="3" xpath="/dfbz"></Field>
		<Field id="jfll" desc="借方利率" edittype="text" status="F" datatype="big_decimal" size="10" xpath="/jfll"></Field>
		<Field id="dfll" desc="贷方利率" edittype="text" status="F" datatype="big_decimal" size="10" xpath="/dfll"></Field>
		<Field id="bzjjybz" desc="保证金交易标志" edittype="text" status="F" datatype="string" size="4" xpath="/bzjjybz"></Field>
		<Field id="glbzjzh" desc="关联保证金账户" edittype="text" status="F" datatype="string" size="60" xpath="/glbzjzh"></Field>
		<Field id="glywbh" desc="关联业务编号" edittype="text" status="F" datatype="string" size="40" xpath="/glywbh"></Field>
		<Field id="wbglxtmc" desc="外部关联系统名称" edittype="text" status="F" datatype="string" size="100" xpath="/wbglxtmc"></Field>
		<Field id="cjrq" desc="采集日期" edittype="text" status="F" datatype="string" size="8" xpath="/id/cjrq"></Field>
		<Field id="operation" desc="操作" status="N" method="None" xpath=""
			   primary="true" type="" size="20" default="" tip="操作" require=""/>	
       </Fields>
    <Where>
    	<TextBox id="cjrq" desc="采集日期" datatype="date" index="5" require="true" errmsg="数据日期不能为空！" tip="时间为必输项" size="12"/>
    	<TextBox id="yxjgdm" desc="银行机构代码" datatype="string" index="3" size="30"/>
    </Where>
	<Operations>
		<Button id="btModOrAdd" name="btModOrAdd" desc="保存" operation="asysubmit"
			updateClass="resources.east.data.update.EastZcfzkmtjbModOrAddUpdate"
			url="" txn="" />
		<Button id="btDel" name="btDel" desc="删除" operation="asysubmit"
			updateClass="resources.east.data.update.EastZcfzkmtjbModOrAddUpdate"
			url="" />
		<Button id="btCancel" name="btCancel" desc="取消" operation="href" url="" />
		<Button id="btAdd" name="btAdd" desc="新增" operation="" url="#" show="" group="" txn="" icon="icon-add"/>
	</Operations>
</CommQry>