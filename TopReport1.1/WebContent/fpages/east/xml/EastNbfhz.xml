<?xml version="1.0" encoding="UTF-8"?>

<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="内部分户账" navigate="银监标准化    &gt; 内部分户账" type="call"
	interface="false" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true"
	getterclassname="resources.east.data.getter.EastNbfhzGetter">
	<Include id="BankParam" />
	<!--
	<PageQryExp type="CSV" limit="true" encoding="GBK" url="/trans/qryExp.do" directory="F:/upload"/>
	-->
	<PageQryExp type="XLS" limit="true" encoding="GBK" url="/trans/qryExp.do" desc="批量下载-内部分户账" />
	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		
		<Field id="id" desc="id" edittype="text" datatype="string" size="30" xpath="/id"></Field>
        <Field id="nbfhzzh" desc="内部分户账账号" edittype="text"  require="true" status="F" datatype="string" size="60" xpath="/id/nbfhzzh"></Field>
	    <Field id="yxjgdm" desc="银行机构代码" status="F"  require="true" edittype="text" datatype="string" size="30" xpath="/yxjgdm"></Field>	
		<Field id="jrxkzh" desc="金融许可证号" status="F"  require="true"  edittype="text"  datatype="string" size="30" xpath="/jrxkzh"></Field>
		<Field id="nbjgh" desc="内部机构号" status="F"  require="true"  edittype="text"  datatype="string" size="30" xpath="/nbjgh" />
		<Field id="mxkmbh" desc="明细科目编号" status="F"   require="true"  edittype="text"  datatype="string" size="60" xpath="/mxkmbh" />
		<Field id="yxjgmc" desc="银行机构名称" status="F"  require="true"  edittype="text"  datatype="string" size="200" xpath="/yxjgmc" />
		<Field id="mxkmmc" desc="明细科目名称" status="F"  require="true"  edittype="text"  datatype="string" size="300" xpath="/mxkmmc" />
		<Field id="tjkmbh" desc="统计科目编号" status="F"  require="true"  edittype="text"  datatype="string" size="20" xpath="/tjkmbh" />
		<Field id="bz" desc="币种" status="F"  require="true"  edittype="text"  datatype="string" size="3" xpath="/id/bz" />
		<Field id="zhmc" desc="账户名称" status="F"  require="true"  edittype="text"  datatype="string" size="200" xpath="/zhmc" />
		<Field id="nbfhzzhlx" desc="个人活期存款账户类型" status="F"  require="true"  edittype="text"  datatype="string" size="60" xpath="/nbfhzzhlx" />
		<Field id="jdbz" desc="借贷标志" status="F"  require="true"  edittype="select" translated="DATA_DIC.20046"  datatype="string" size="12" xpath="/jdbz" />
		<Field id="ll" desc="利率" status="F"   require="true" edittype="text"  datatype="string" size="10" xpath="/ll" />
		<Field id="jfye" desc="借方余额" status="F"  require="true"  edittype="text"  datatype="currency" size="20" xpath="/jfye" />
		<Field id="dfye" desc="贷方余额" status="F"  require="true"  edittype="text"  datatype="currency" size="20" xpath="/dfye" />
		<Field id="jxbz" desc="计息标志" status="F"  require="true"  edittype="select" translated="DATA_DIC.20034"  datatype="string" size="4" xpath="/jxbz" />
		<Field id="jxfs" desc="计息方式" status="F"   require="true" edittype="text"  datatype="string" size="30" xpath="/jxfs" />
		<Field id="khrq" desc="开户日期" status="F"  require="true"  edittype="text"  datatype="string" size="8" xpath="/khrq" />
		<Field id="xhrq" desc="销户日期" status="F"  require="true"  edittype="text"  datatype="string" size="8" xpath="/xhrq" />
		<Field id="zhzt" desc="账户状态" status="F"  require="true"  edittype="text"  datatype="string" size="60" xpath="/zhzt" />
		<Field id="cjrq" desc="采集日期" status="F"  require="true"  edittype="text"  datatype="string" size="8" xpath="/id/cjrq" />
		<Field id="operation" desc="操作" status="N" method="None" xpath=""
			   primary="true" type="" size="20" default="" tip="操作" require=""/>	
       </Fields>
	<Where>
    	<TextBox id="cjrq" desc="采集日期" datatype="date" index="5" require="true" errmsg="数据日期不能为空！" tip="时间为必输项" size="12"/>
    	<TextBox id="yxjgdm" desc="银行机构代码" datatype="string" index="3" size="30"/>
    </Where>
	<Operations>
		<Button id="btModOrAdd" name="btModOrAdd" desc="保存" operation="asysubmit"
			updateClass="resources.east.data.update.EastNbfhzModOrAddUpdate"
			url="" txn="" />
		<Button id="btDel" name="btDel" desc="删除" operation="asysubmit"
			updateClass="resources.east.data.update.EastNbfhzModOrAddUpdate"
			url="" />
		<Button id="btCancel" name="btCancel" desc="取消" operation="href" url="" />
		<Button id="btAdd" name="btAdd" desc="新增" operation="" url="#" show="" group="" txn="" icon="icon-add"/>
	</Operations>
</CommQry>