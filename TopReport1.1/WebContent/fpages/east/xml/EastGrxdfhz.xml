<CommQry title="个人信贷分户账" navigate="银监标准化    &gt; 个人信贷分户账" type="call"
	interface="false" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true"
	getterclassname="resources.east.data.getter.EastGrxdfhzGetter">
	<Include id="BankParam" />
	<!--
	<PageQryExp type="CSV" limit="true" encoding="GBK" url="/trans/qryExp.do" directory="F:/upload"/>
	-->
	<PageQryExp type="XLS" limit="true" encoding="GBK" url="/trans/qryExp.do" desc="批量下载-个人信贷分户账" />
	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		
		<Field id="id" desc="id" edittype="text" datatype="string" size="30" xpath="/id"></Field>
        <Field id="dkfhzh" desc="贷款分户账号"  require="true" edittype="text" status="F" datatype="string" size="60" xpath="/id/dkfhzh"></Field>
        <Field id="xdjjh" desc="信贷借据号"  require="true" edittype="text" status="F" datatype="string" size="100" xpath="/id/xdjjh"></Field>	
	    <Field id="khtybh" desc="客户统一编号"  require="true" edittype="text" status="F" datatype="string" size="60" xpath="/khtybh"></Field>
	    <Field id="yxjgdm" desc="银行机构代码"  require="true" status="F" edittype="text" datatype="string" size="30" xpath="/yxjgdm"></Field>	
		<Field id="jrxkzh" desc="金融许可证号"  require="true" status="F"  edittype="text"  datatype="string" size="30" xpath="/jrxkzh"></Field>
		<Field id="nbjgh" desc="内部机构号"  require="true" status="F"  edittype="text"  datatype="string" size="30" xpath="/nbjgh" />
		<Field id="mxkmbh" desc="明细科目编号"  require="true" status="F"  edittype="text"  datatype="string" size="60" xpath="/mxkmbh" />
		<Field id="yxjgmc" desc="银行机构名称"  require="true" status="F"  edittype="text"  datatype="string" size="200" xpath="/yxjgmc" />
		<Field id="mxkmmc" desc="明细科目名称"  require="true" status="F"  edittype="text"  datatype="string" size="300" xpath="/mxkmmc" />
		<Field id="tjkmbh" desc="统计科目编号"  require="true" status="F"  edittype="text"  datatype="string" size="20" xpath="/tjkmbh" />
		<Field id="bz" desc="币种"  require="true" status="F"  edittype="text"  datatype="string" size="3" xpath="/id/bz" />
		<Field id="zhmc" desc="账户名称"  require="true" status="F"  edittype="text"  datatype="string" size="200" xpath="/zhmc" />
		<Field id="zjhkrq" desc="上次还款日期"  require="true" status="F"  edittype="text"  datatype="string" size="8" xpath="/zjhkrq" />
		<Field id="dkhth" desc="贷款合同号" require="true"  status="F"  edittype="text"  datatype="string" size="100" xpath="/dkhth" />
		<Field id="xdyxm" desc="信贷员姓名" require="true"  status="F"  edittype="text"  datatype="string" size="100" xpath="/xdyxm" />
		<Field id="dkwjfl" desc="贷款五级分类"  require="true" status="F"  edittype="select" translated="DATA_DIC.20047"  datatype="string" size="6" xpath="/dkwjfl" />
		<Field id="hkzh" desc="还款账户"  require="true" status="F"  edittype="text"  datatype="string" size="60" xpath="/hkzh" />
		<Field id="dkrzzh" desc="贷款入账账户"  require="true" status="F"  edittype="text"  datatype="string" size="60" xpath="/dkrzzh" />
		<Field id="dkll" desc="贷款利率"  require="true" status="F"  edittype="text"  datatype="string" size="10" xpath="/dkll" />
		<Field id="dkbjze" desc="贷款本金总额"  require="true" status="F"  edittype="text"  datatype="currency" size="20" xpath="/dkbjze" />
		<Field id="dkzcye" desc="贷款正常余额"  require="true" status="F"  edittype="text"  datatype="currency" size="20" xpath="/dkzcye" />
		<Field id="dkyqye" desc="贷款逾期余额"  require="true" status="F"  edittype="text"  datatype="currency" size="20" xpath="/dkyqye" />
		<Field id="qbye" desc="欠本余额" require="true"  status="F"  edittype="text"  datatype="currency" size="20" xpath="/qbye" />
		<Field id="bwqxye" desc="表外欠息余额"  require="true" status="F"  edittype="text"  datatype="currency" size="20" xpath="/bwqxye" />
		<Field id="dkqx" desc="贷款期限"  require="true" status="F"  edittype="text"  datatype="string" size="8" xpath="/dkqx" />
		<Field id="dqrq" desc="到期日期"  require="true" status="F"  edittype="text"  datatype="string" size="8" xpath="/dqrq" />
		<Field id="qxrq" desc="起息日期" require="true"  status="F"  edittype="text"  datatype="string" size="8" xpath="/qxrq" />
		<Field id="khrq" desc="开户日期" require="true"  status="F"  edittype="text"  datatype="string" size="8" xpath="/khrq" />
		<Field id="xhrq" desc="销户日期"  require="true" status="F"  edittype="text"  datatype="string" size="8" xpath="/xhrq" />
		<Field id="yqrq" desc="逾期日期"  require="true" status="F"  edittype="text"  datatype="string" size="8" xpath="/yqrq" />
		<Field id="zhzt" desc="账户状态"  require="true" status="F"  edittype="text"  datatype="string" size="60" xpath="/zhzt" />
		<Field id="cjrq" desc="采集日期"  require="true" status="F"  edittype="text"  datatype="string" size="8" xpath="/id/cjrq" />
		<Field id="operation" desc="操作" status="N" method="None" xpath=""
			   primary="true" type="" size="20" default="" tip="操作" require=""/>	
       </Fields>
	<Where>
    	<TextBox id="cjrq" desc="采集日期" datatype="date" index="5" require="true" errmsg="数据日期不能为空！" tip="时间为必输项" size="12"/>
    	<TextBox id="yxjgdm" desc="银行机构代码" datatype="string" index="3" size="30"/>
    	<TextBox id="dkfhzh" desc="贷款分户账号" datatype="string" index="3" size="60"/>
    	<TextBox id="xdjjh" desc="信贷借据号" datatype="string" index="3" size="100"/>
    </Where>
	<Operations>
		<Button id="btModOrAdd" name="btModOrAdd" desc="保存" operation="asysubmit"
			updateClass="resources.east.data.update.EastGrxdfhzModOrAddUpdate"
			url="" txn="" />
		<Button id="btDel" name="btDel" desc="删除" operation="asysubmit"
			updateClass="resources.east.data.update.EastGrxdfhzModOrAddUpdate"
			url="" />
		<Button id="btCancel" name="btCancel" desc="取消" operation="href" url="" />
		<Button id="btAdd" name="btAdd" desc="新增" operation="" url="#" show="" group="" txn="" icon="icon-add"/>
	</Operations>
</CommQry>