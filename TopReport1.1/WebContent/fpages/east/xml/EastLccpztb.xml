<?xml version="1.0" encoding="UTF-8"?>

<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="理财产品状态表" navigate="银监标准化    &gt; 理财产品状态表" type="call"
	interface="false" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true"
	getterclassname="resources.east.data.getter.EastLccpztbGetter">
	<Include id="BankParam" />
	<!--
	<PageQryExp type="CSV" limit="true" encoding="GBK" url="/trans/qryExp.do" directory="F:/upload"/>
	-->
	<PageQryExp type="XLS" limit="true" encoding="GBK" url="/trans/qryExp.do" desc="理财产品状态表" />
	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		
		<Field id="id" desc="行内标识码" require="true"  edittype="text" status="F" datatype="string" size="30" xpath="/id"></Field>
		<Field id="yxjgdm" desc="银行机构代码"  require="true" edittype="text" status="F" datatype="string" size="30" xpath="/yxjgdm"></Field>
		<Field id="jrxkzh" desc="金融许可证号"  require="true" edittype="text" status="F" datatype="string" size="30" xpath="/jrxkzh"></Field>
		<Field id="nbjgh" desc="内部机构号"  require="true" edittype="text" status="F" datatype="string" size="30" xpath="/nbjgh"></Field>
		<Field id="yxjgmc" desc="银行机构名称" require="true"  edittype="text" status="F" datatype="string" size="200" xpath="/yxjgmc"></Field>
		<Field id="lccpmc" desc="理财产品名称" require="true"  edittype="text" status="F" datatype="string" size="200" xpath="/lccpmc"></Field>
		<Field id="sjmjje" desc="实际募集金额" require="true"  edittype="text" status="F" datatype="currency" size="20" xpath="/sjmjje"></Field>
		<Field id="cpqsrq" desc="产品起始日期" require="true"  edittype="text" status="F" datatype="string" size="8" xpath="/cpqsrq"></Field>
		<Field id="cpyjzzrq" desc="产品预计终止日期" require="true"  edittype="text" status="F" datatype="string" size="8" xpath="/cpyjzzrq"></Field>
		<Field id="fxdjr" desc="发行登记日"  require="true" edittype="text" status="F" datatype="string" size="8" xpath="/fxdjr"></Field>
		<Field id="yhdsjsxsr" desc="银行端实际实现收入"  require="true" edittype="text" status="F" datatype="currency" size="20" xpath="/yhdsjsxsr"></Field>
		<Field id="dfkhsy" desc="兑付客户收益" require="true"  edittype="text" status="F" datatype="currency" size="20" xpath="/dfkhsy"></Field>
		<Field id="khdsjnhsyl" desc="客户端实际年化收益率"  require="true" edittype="text" status="F" datatype="big_decimal" size="10" xpath="/khdsjnhsyl"></Field>
		<Field id="cpdsjnhsyl" desc="产品端实际年化收益率" require="true"  edittype="text" status="F" datatype="big_decimal" size="10" xpath="/cpdsjnhsyl"></Field>
		<Field id="zzdjr" desc="终止登记日"  require="true" edittype="text" status="F" datatype="string" size="8" xpath="/zzdjr"></Field>
		<Field id="cjrq" desc="采集日期"  require="true" edittype="text" status="F" datatype="string" size="8" xpath="/cjrq"></Field>
		<Field id="operation" desc="操作" status="N" method="None" xpath=""
			   primary="true" type="" size="20" default="" tip="操作" require=""/>	
       </Fields>
    <Where>
    	<TextBox id="cjrq" desc="采集日期" datatype="date" index="5" require="true" errmsg="数据日期不能为空！" tip="时间为必输项" size="12"/>
    	<TextBox id="id" desc="行内标识码" datatype="string" index="3" size="30"/>
    	<TextBox id="lccpmc" desc="理财产品名称" datatype="string" index="3" size="200"/>
    	<TextBox id="nbjgh" desc="内部机构号" datatype="string" index="3" size="30"/>
    </Where>
	<Operations>
		<Button id="btModOrAdd" name="btModOrAdd" desc="保存" operation="asysubmit"
			updateClass="resources.east.data.update.EastLccpztbModOrAddUpdate"
			url="" txn="" />
		<Button id="btDel" name="btDel" desc="删除" operation="asysubmit"
			updateClass="resources.east.data.update.EastLccpztbModOrAddUpdate"
			url="" />
		<Button id="btCancel" name="btCancel" desc="取消" operation="href" url="" />
		<Button id="btAdd" name="btAdd" desc="新增" operation="" url="#" show="" group="" txn="" icon="icon-add"/>
	</Operations>
</CommQry>