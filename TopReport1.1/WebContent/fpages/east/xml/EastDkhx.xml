<?xml version="1.0" encoding="UTF-8"?>

<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="贷款核销" navigate="银监标准化    &gt; 贷款核销" type="call"
	interface="false" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true"
	getterclassname="resources.east.data.getter.EastDkhxGetter">
	<Include id="BankParam" />
	<!--
	<PageQryExp type="CSV" limit="true" encoding="GBK" url="/trans/qryExp.do" directory="F:/upload"/>
	-->
	<PageQryExp type="XLS" limit="true" encoding="GBK" url="/trans/qryExp.do" desc="批量下载-贷款核销" />
	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		
		
		<Field id="id" desc="id" edittype="text" datatype="string" size="30" xpath="/id"></Field>
		<Field id="xdjjh" desc="信贷借据号" status="F"  edittype="text"  datatype="string" size="100" xpath="/id/xdjjh" />
		<Field id="xdhth" desc="信贷合同号" status="F"  edittype="text"  datatype="string" size="100" xpath="/xdhth" />
		<Field id="khtybh" desc="客户统一编号" status="F"  edittype="text"  datatype="string" size="60" xpath="/khtybh" />
		<Field id="jrxkzh" desc="金融许可证号" edittype="text" status="F" datatype="string" size="30" xpath="/jrxkzh"></Field>
		<Field id="yxjgdm" desc="银行机构代码" status="F"  edittype="text"  datatype="string" size="30" xpath="/yxjgdm"></Field>
		<Field id="nbjgh" desc="内部机构号 " status="F"  edittype="text"  datatype="string" size="30" xpath="/nbjgh" />
		<Field id="mxkmbh" desc="明细科目编号" status="F"  edittype="text"  datatype="string" size="60" xpath="/mxkmbh" />
	    <Field id="mxkmmc" desc="明细科目名称" status="F" edittype="text" datatype="string" size="300" xpath="/mxkmmc"></Field>	
		<Field id="kmgsjg" desc="科目归属机构" status="F"  edittype="text"  datatype="string" size="30" xpath="/kmgsjg" />
		<Field id="bz" desc="币种" status="F"  edittype="text"  datatype="string" size="3" xpath="/bz" />
		<Field id="shdkbj" desc="实核贷款本金" status="F"  edittype="text"  datatype="string" size="24" xpath="/shdkbj" />
		<Field id="shbnlx" desc="实核表内利息" status="F"  edittype="text"  datatype="string" size="24" xpath="/shbnlx" />
		<Field id="shbwlx" desc="实核表外利息" status="F"  edittype="text"  datatype="string" size="24" xpath="/shbwlx" />
		<Field id="hxrq" desc="核销日期" status="F"  edittype="text"  datatype="string" size="8" xpath="/id/hxrq" />
		<Field id="hxshbj" desc="核销收回本金" status="F"  edittype="text"  datatype="string" size="24" xpath="/hxshbj" />
		<Field id="hxshbnlx" desc="核销收回表内利息" status="F"  edittype="text"  datatype="string" size="24" xpath="/hxshbnlx" />
		<Field id="hxshbwlx" desc="核销收回表外利息" status="F"  edittype="text"  datatype="string" size="24" xpath="/hxshbwlx" />
		<Field id="shbz" desc="收回标志" status="F"  edittype="text"  datatype="string" size="3" xpath="/shbz" />
		<Field id="hxshgyh" desc="核销收回柜员号" status="F"  edittype="text"  datatype="string" size="30" xpath="/hxshgyh" />
		<Field id="hxshrq" desc="核销收回日期" status="F"  edittype="text"  datatype="string" size="8" xpath="/id/hxshrq" />
		<Field id="bzh" desc="备注" status="F"  edittype="text"  datatype="string" size="200" xpath="/bzh" />
		<Field id="cjrq" desc="采集日期" status="F"  edittype="text"  datatype="string" size="8" xpath="/id/cjrq" />
		<Field id="operation" desc="操作" status="N" method="None" xpath=""
			   primary="true" type="" size="20" default="" tip="操作" require=""/>	
       </Fields>
	<Where>
    	<TextBox id="cjrq" desc="采集日期" datatype="date" index="5" require="true" errmsg="数据日期不能为空！" tip="时间为必输项" size="12"/>
    	<TextBox id="yxjgdm" desc="银行机构代码" datatype="string" index="3" size="30"/>
    </Where>
	<Operations>
		<Button id="btModOrAdd" name="btModOrAdd" desc="保存" operation="asysubmit"
			updateClass="resources.east.data.update.EastDkhxModOrAddUpdate"
			url="" txn="" />
		<Button id="btDel" name="btDel" desc="删除" operation="asysubmit"
			updateClass="resources.east.data.update.EastDkhxModOrAddUpdate"
			url="" />
		<Button id="btCancel" name="btCancel" desc="取消" operation="href" url="" />
		<Button id="btAdd" name="btAdd" desc="新增" operation="" url="#" show="" group="" txn="" icon="icon-add"/>
	</Operations>
</CommQry>