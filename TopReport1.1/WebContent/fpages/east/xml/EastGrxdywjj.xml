		<Field id="bnqxye" desc="表内欠息余额"  require="true" status="F"  edittype="text"  datatype="currency" size="24" xpath="/bnqxye" />
		<Field id="bwqxye" desc="表外欠息余额"  require="true" status="F"  edittype="text"  datatype="currency" size="24" xpath="/bwqxye" />
		<Field id="dkzt" desc="贷款状态"  require="true" status="F"  edittype="text"  datatype="string" size="100" xpath="/dkzt" />
