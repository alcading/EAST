	getterclassname="resources.east.data.getter.EastXdywzhdywGetter">
	<Include id="BankParam" />
	<!--
	<PageQryExp type="CSV" limit="true" encoding="GBK" url="/trans/qryExp.do" directory="F:/upload"/>
	-->
	<PageQryExp type="XLS" limit="true" encoding="" url="/trans/qryExp.do" desc="批量下载-信贷业务质或抵押物" />
	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		
		
		<Field id="id" desc="id" edittype="text" datatype="string" size="30" xpath="/id"></Field>
		<Field id="zhdywbh" desc="质或抵押物编号"  require="true" status="F"  edittype="text"  datatype="string" size="60" xpath="/id/zhdywbh" />
		<Field id="dbhth" desc="担保合同号"  require="true" status="F"  edittype="text"  datatype="string" size="100" xpath="/dbhth" />
		<Field id="khtybh" desc="客户统一编号"  require="true" edittype="text" status="F" datatype="string" size="60" xpath="/khtybh"></Field>
		<Field id="yxjgdm" desc="银行机构代码"  require="true" status="F"  edittype="text"  datatype="string" size="30" xpath="/yxjgdm"></Field>
		<Field id="jrxkzh" desc="金融许可证号"  require="true" status="F"  edittype="text"  datatype="string" size="30" xpath="/jrxkzh" />
		<Field id="nbjgh" desc="内部机构号" require="true"  status="F"  edittype="text"  datatype="string" size="30" xpath="/nbjgh" />
		<Field id="zhdywmc" desc="质或抵押物名称"  require="true" status="F"  edittype="text"  datatype="string" size="1000" xpath="/zhdywmc" />
		<Field id="zhdywlx" desc="质或抵押物类型"  require="true" status="F"  edittype="text"  datatype="string" size="300" xpath="/zhdywlx" />
	    <Field id="zhdywzmjz" desc="质或抵押物账面价值"  require="true" status="F" edittype="text" datatype="currency" size="24" xpath="/zhdywzmjz"></Field>	
		<Field id="bz" desc="币种"  require="true" status="F"  edittype="text"  datatype="string" size="3" xpath="/bz" />
		<Field id="yxrdjz" desc="银行认定价值"  require="true" status="F"  edittype="text"  datatype="currency" size="24" xpath="/yxrdjz" />
		<Field id="pgjz" desc="评估价值"  require="true" status="F"  edittype="text"  datatype="currency" size="24" xpath="/pgjz" />
		<Field id="pgrq" desc="评估日期"  require="true" status="F"  edittype="text"  datatype="string" size="8" xpath="/pgrq" />
		<Field id="pgjgmc" desc="评估机构名称"  require="true" status="F"  edittype="text"  datatype="string" size="200" xpath="/pgjgmc" />
		<Field id="zhdyl" desc="质或抵押率"  require="true" status="F"  edittype="text"  datatype="string" size="14" xpath="/zhdyl" />
		<Field id="dywsyqrmc" desc="抵押物所有权人名称"  require="true" status="F"  edittype="text"  datatype="string" size="200" xpath="/dywsyqrmc" />
		<Field id="ydyjz" desc="已抵押价值"  require="true" status="F"  edittype="text"  datatype="currency" size="24" xpath="/ydyjz" />
		<Field id="djrq" desc="登记日期"  require="true" status="F"  edittype="text"  datatype="string" size="8" xpath="/djrq" />
		<Field id="swsqrq" desc="实物收取日期"  require="true" status="F"  edittype="text"  datatype="string" size="8" xpath="/swsqrq" />
