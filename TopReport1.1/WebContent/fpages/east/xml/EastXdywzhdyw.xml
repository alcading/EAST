		<Field id="zypzzh" desc="质押票证账号" require="true"  status="F"  edittype="text"  datatype="string" size="60" xpath="/zypzzh" />
		<Field id="zypzlx" desc="质押票证类型"  require="true" status="F"  edittype="text"  datatype="string" size="60" xpath="/zypzlx" />
		<Field id="zypzhm" desc="质押票证号码"  require="true" status="F"  edittype="text"  datatype="string" size="300" xpath="/zypzhm" />
		<Field id="zypzje" desc="质押票证金额"  require="true" status="F"  edittype="text"  datatype="currency" size="24" xpath="/zypzje" />
