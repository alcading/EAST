		<Field id="hbrq" desc="核保日期" require="true"  status="F"  edittype="text"  datatype="string" size="8" xpath="/hbrq" />
		<Field id="hbryxm" desc="核保人一姓名"  require="true" status="F"  edittype="text"  datatype="string" size="100" xpath="/hbryxm" />
		<Field id="hbrexm" desc="核保人二姓名"  require="true" status="F"  edittype="text"  datatype="string" size="100" xpath="/hbrexm" />
		<Field id="qzdjhm" desc="权证登记号码"  require="true" status="F"  edittype="text"  datatype="string" size="300" xpath="/qzdjhm" />
		<Field id="qzmc" desc="权证名称" require="true"  status="F"  edittype="text"  datatype="string" size="200" xpath="/qzmc" />
		<Field id="qzyxdqrq" desc="权证有效到期日期"  require="true" status="F"  edittype="text"  datatype="string" size="8" xpath="/qzyxdqrq" />
