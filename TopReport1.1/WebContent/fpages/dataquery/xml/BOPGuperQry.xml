	           size="" default="" xpath="/guperdate"  tip=""/>

		<Field id="guperamount" desc="履约金额" status="F" require="" edittype="text" rules="isRate_22_2"   datatype="currency" scale="2" size="22" method="None"
	           default="" xpath="/guperamount"  tip=""/>

	    <Field id="bencode" desc="受益人代码" require="" edittype="text" datatype="string" method="None"
	 	       readonly="false" size="" status="F" default="" xpath="/bencode"/>

	 	<Field id="bename" desc="受益人中文名称" require="" edittype="text" datatype="string" method="None"
	 	       readonly="false" size="" status="F" default="" xpath="/bename"/>

	 	<Field id="benamen" desc="受益人英文名称" require="" edittype="text" datatype="string" method="None"
	 	       readonly="false" size="" status="F" default="" xpath="/benamen"/>

	    <Field id="opr" desc="操作" require="" edittype="text" datatype="string" method="None"
	 	       readonly="false" size="" status="N" default="" xpath="/opr"/>
