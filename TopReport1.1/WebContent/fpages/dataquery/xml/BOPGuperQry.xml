         <Field id="gupercurrName" desc="履约币种" status="N" require="" method="com.huateng.ebank.business.common.CQMethod.getCurNameByCurNo" width="" readonly="" primary="true" type="String" size="150" init="false"
			xpath="/gupercurr" default=""/>
