	<Where>
        <DateTextBox id="qWorkDateStart" desc="开始工作日期" tip="开始工作日期" require="false" status="F" datatype="date" method="None" primary="false" index="0"  />

	    <DateTextBox id="qWorkDateEnd" desc="结束工作日期" tip="结束工作日期" require="false" status="F" datatype="date" method="None" primary="false" index="1"  />

		<Option id="qBrNo" operation="EQ" size="" desc="所属机构" tip="机构号" type="string" translated="CQ:BctlAllSelect"
				fieldmap="qBrNo=brno;qBrNoName=brcodeTypeName" viewField="brcodeTypeName" dropdowntype="dynamic" init="true"
				require="false" readOnly="" index="2"/>
		<Option id="qActiontype" desc="操作类型" require="false" edittype="select" status="F"  translated="DATA_DIC.10" size="1" datatype="string"   index="3"/>

		<Option id="qRecStatus" desc="记录状态" require="false" edittype="select" size="2" status="N" translated="DATA_DIC.62" datatype="string"   index="4"/>

		<Option id="qApproveStatus" desc="审核状态" require="false" edittype="select" size="2" status="N" translated="DATA_DIC.64" datatype="string" default="" xpath="/notCollectFlag" index="5"/>

		<Option id="qRepStatus" desc="回执状态" require="false" edittype="select" size="2" status="N" translated="DATA_DIC.63" datatype="string" default=""  index="6"/>

		<TextBox id="qFiller2" desc="业务流水号" tip="银行业务流水号" require="false" status="F" edittype="text"  datatype="string" size="28" method="None" primary="false" index="7" />

    </Where>
