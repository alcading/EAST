	 	<Field id="repStatus" desc="回执状态" status="N" require="false" edittype="select" size="2" translated="DATA_DIC.63" datatype="string" default="" xpath="/repStatus" tip="回执状态" />

	 	<Field id="approveStatus" desc="审核状态" status="N" require="false" edittype="select" size="2" translated="DATA_DIC.64" datatype="string" default="" xpath="/approveStatus" tip="审核状态" />

	 	<Field id="recStatus" desc="记录状态" status="N" require="false" edittype="select" size="2" translated="DATA_DIC.62" datatype="string" default="" xpath="/recStatus" tip="记录状态" />

	 	<Field id="pguperamount" desc="购汇履约金额" status="F" require="" edittype="text" rules="isRate_22_2"   datatype="currency" scale="2" size="22"   xpath="/pguperamount" />

