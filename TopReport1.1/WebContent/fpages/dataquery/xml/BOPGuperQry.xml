	           size="" default="" xpath="/apptype" tip=""/>

	    <Field id="maindebtcurr" desc="主债务币种" require="" edittype="text" datatype="string"
	           size="" default="" xpath="/maindebtcurr"  tip="/maindebtcurr"/>

		<Field id="guarancurr" desc="保函币种" status="N" require="false" edittype="text" datatype="string" method="None"
	           size="" default="" xpath="/guarancurr"  tip=""/>

	    <Field id="actiondesc" desc="删除原因" status="F" require="false" edittype="text" datatype="string"
	           size="" default="" xpath="/actiondesc"  tip=""/>

        <Field id="basere" desc="担保责任余额" status="N" require="false" edittype="text" datatype="string"
               size="" default="" xpath="/basere" tip="" />

	    <Field id="id" desc="记录主键" require="" status="N"  edittype="text" datatype="string" method="None"
	           size="" default="" xpath="id" tip=""/>

