<CommQry title="国内外汇贷款变动信息" navigate="上报数据 &gt; 国内外汇贷款变动信息 &gt; 变动信息"
	type="call"
	interface="false"
	pagesize="10"
	async="true"
	databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	getterclassname="com.huateng.report.dataquery.getter.BopCfaDofoexloDsChangeInfoQueryGetter">
	<Include id="BankParam" />

	<PageQryExp type="XLS" limit="true" encoding="" url="/trans/qryExp.do" desc="批量下载-国内外汇贷款变动信息"  />

	<Fields>
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传  -->

		<Field id="id" desc="记录主键" require="" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="N"
			default="" primary="true" xpath="/id" />

		<Field id="actiontype" desc="操作类型" require="true" edittype="select"
			datatype="string" method="None" readonly="false" size="1" status="F"
			default="" xpath="/actiontype" translated="DATA_DIC.10" />

		<Field id="actiondesc" desc="删除原因" require="" edittype="text"
			datatype="string" method="None" readonly="false" size="128"
			status="F" default="" xpath="/actiondesc" />


		<!-- 现在用于记录签约信息的ID -->
		<Field id="filler2" desc="业务流水号" require="true" edittype="text"
			datatype="string" method="None" readonly="false" size="64" status="F"
			default="" xpath="/filler2" />


		<Field id="dofoexlocode" desc="国内外汇贷款编号" require="true"
			edittype="text" datatype="string" method="None" readonly="false"
			size="28" status="F" default="" xpath="/dofoexlocode" />

		<Field id="buscode" desc="银行业务参号" require="true" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" xpath="/buscode" />



		<Field id="changeno" desc="变动编号" require="true" edittype="text"
			datatype="string" method="None" readonly="false" size="4" status="F"
			default="" xpath="/changeno" />

		<Field id="changedate" desc="变动日期" require="true" edittype="string" datatype="date" method="None"
	 	  	readonly="false" size="8" status="F" default="" xpath="/changedate" />



		<Field id="loanopenbalan" desc="期初余额" require="true" edittype="text" datatype="currency" method="None" readonly="false"
	 		size="22" status="F" default="" xpath="/loanopenbalan" />

	 	<Field id="endbalan" desc="期末余额" require="true" edittype="text"
			datatype="currency" method="None" readonly="false" size="22" scale="2"
			status="F" default="" xpath="/endbalan" />



		<Field id="withcurrence" desc="提款币种" require=""
			edittype="select" datatype="string" method="None" readonly="false"
			size="3" status="N" default="" translated="CQ:CurrencySelect"
			fieldmap="withcurrence=currencyNo;withcurrenceName=currencyNoName"
			viewField="currencyNoName" xpath="/withcurrence" />
		<Field id="withcurrenceName" desc="提款币种" status="F"
			method="com.huateng.ebank.business.common.CQMethod.getCurNameByCurNo"
			width="" readonly="" primary="true" type="String" size="150"
			xpath="/withcurrence" default=""/>

		<Field id="withamount" desc="提款金额" require="" edittype="text"
			datatype="currency" method="None" readonly="false" size="22"
			status="F" default="" xpath="/withamount" />



		<Field id="settamount" desc="结汇金额" require="" edittype="text"
			datatype="currency" scale="2" method="None" readonly="false"
			size="22" status="F" default="" xpath="/settamount" />



		<Field id="useofunds" desc="资金用途" require="" edittype="select"
			datatype="string" method="None" readonly="false" size="2"
			status="F" default="" xpath="/useofunds" translated="DATA_DIC.47" />



		<Field id="princurr" desc="还本币种" require="" edittype="select"
			datatype="string" method="None" readonly="false" size="3" status="N"
			default="" translated="CQ:CurrencySelect"
			fieldmap="princurr=currencyNo;princurrName=currencyNoName"
			viewField="currencyNoName" xpath="/princurr" />
		<Field id="princurrName" desc="还本币种" status="F"
			method="com.huateng.ebank.business.common.CQMethod.getCurNameByCurNo"
			width="" readonly="" primary="true" type="String" size="150"
			xpath="/princurr" default=""/>

		<Field id="repayamount" desc="还本金额" require="" edittype="text"
			datatype="currency" method="None" readonly="false" size="22"
			status="F" default="" xpath="/repayamount" />

		<Field id="prepayamount" desc="购汇还本金额" require=""
			edittype="text" datatype="currency" method="None" readonly="false"
			size="22" status="F" default="" xpath="/prepayamount" />



		<Field id="inpaycurr" desc="付息币种" require="" edittype="select"
			datatype="string" method="None" readonly="false" size="3" status="N"
			default="" translated="CQ:CurrencySelect"
			fieldmap="inpaycurr=currencyNo;inpaycurrName=currencyNoName"
			viewField="currencyNoName" xpath="/inpaycurr" />
		<Field id="inpaycurrName" desc="付息币种" status="F"
			method="com.huateng.ebank.business.common.CQMethod.getCurNameByCurNo"
			width="" readonly="" primary="true" type="String" size="150"
			xpath="/inpaycurr" default=""/>

		<Field id="inpayamount" desc="付息金额" require="" edittype="text"
			datatype="currency" method="None" readonly="false" size="22"
			status="F" default="" xpath="/inpayamount" />

		<Field id="pinpayamount" desc="购汇付息金额" require=""
			edittype="text" datatype="currency" method="None" readonly="false"
			size="22" status="F" default="" xpath="/pinpayamount" />



		<Field id="remark" desc="备注" require="" edittype="textarea"
			datatype="string" method="None" readonly="false" size="256"
			status="F" default="" xpath="/remark" />



		<Field id="lstUpdTm" desc="最后更新时间" require="" edittype="date"
			datatype="timestamp" method="None" readonly="false" size="32" status="N"
			default="" xpath="/lstUpdTm" />

		<Field id="crtTm" desc="创建时间" require="" edittype="date"
			datatype="timestamp" method="None" readonly="false" size="32" status="N"
			default="" xpath="/crtTm" />


	 	<Field id="brNo" desc="机构号" require="" edittype="text" datatype="string" method="None"
			readonly="false" size="32" status="F" default="" xpath="/brNo" />
		<Field id="brNoName" desc="机构名称" status="N"
			method="com.huateng.ebank.business.common.CQMethod.getBrnoName"
			width="" readonly="" primary="true" type="String" size="150"
			xpath="/brNo" default=""/>

		<Field id="recStatus" desc="记录状态" require="" edittype="select"
			datatype="string" method="None" readonly="false" size="2" status="N"
			default="" xpath="/recStatus"
			translated="DATA_DIC.62" />

		<Field id="repStatus" desc="回执状态" require="" edittype="select"
			datatype="string" method="None" readonly="false" size="2" status="N"
			default="" xpath="/repStatus"
			translated="DATA_DIC.63" />

		<Field id="approveStatus" desc="审核状态" require=""
			edittype="select" datatype="string" method="None" readonly="false"
			size="2" status="N" default="" xpath="/approveStatus"
			translated="DATA_DIC.64" />

		<Field id="approveResult" desc="审核说明" require="" edittype="text"
			datatype="string" method="None" readonly="false" size="256" status="N"
			default="" xpath="/approveResult" />

		<Field id="workDate" desc="工作日期" require="" edittype="date"
			datatype="date" method="None" readonly="false" size="8" status="N"
			default="" xpath="/workDate" />

		<Field id="subSuccess" desc="是否已成功上报" require="" edittype="text"
			datatype="string" method="None" readonly="false" size="1" status="N"
			default="" xpath="/subSuccess" />


	</Fields>

	<Where>

		<DateTextBox id="startDate" desc="开始工作日期" tip="开始工作日期"
			require="false" status="F" datatype="date" method="None" size="150" width=""
			primary="false" index="0" type="String" />

		<DateTextBox id="endDate" desc="结束工作日期" tip="结束工作日期"
			require="false" status="F" datatype="date" method="None" size="150" width=""
			primary="false" index="1" type="String" />

		<Option id="qbrNo" operation="EQ" size=""
			desc="所属机构" tip="机构号" type="string" translated="CQ:BctlAllSelect"
			fieldmap="qbrNo=brno;qbrNoName=brcodeTypeName"
			viewField="brcodeTypeName" dropdowntype="dynamic" init="true"
			require="false" readOnly="" index="2"/>

		<TextBox id="filler2" desc="业务流水号" edittype="text"
			status="F" datatype="string" size="28" index="3" />

		<Option id="actiontype" desc="操作类型" edittype="select"
			datatype="string" size="1" index="4" translated="DATA_DIC.10" />

		<Option id="recStatus" desc="记录状态" edittype="select"
			datatype="string" size="2" index="5"
			translated="DATA_DIC.62" />

		<Option id="approveStatus" desc="审核状态" edittype="select"
			datatype="string" size="2" index="6"
			translated="DATA_DIC.64" />

		<Option id="repStatus" desc="回执状态" edittype="select"
			datatype="string" size="2" index="7"
			translated="DATA_DIC.63" />

	</Where>

	<Operations>

	</Operations>

	<Sql>
	</Sql>
</CommQry>