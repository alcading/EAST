<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.pub">
	<class
		name="SystemBszq"
		table="system_bszq"
		lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>

		<id
			name="id"
			column="ID"
			type="string"
		>
		<generator class="assigned"/>
		</id>
		<property
			name="systemName"
			column="SYSTEM_NAME"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="bszq"
			column="BSZQ"
			type="string"
			not-null="false"
			length="2"
		/>


	</class>
</hibernate-mapping>